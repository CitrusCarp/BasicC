#include <stdio.h>
struct student
{
    int age;
    char name[20];
    int marks;
}S1;
main()
{
    
    printf("enter age=");
    scanf("%d",&S1.age);
    printf("enter marks=");
    scanf("%d",&S1.marks);
    printf("enter name=");
    scanf("%s",S1.name);
    printf("Details of student\n");
    printf("name=%s\n",S1.name);
    printf("age=%d\n",S1.age);
    printf("marks=%d",S1.marks);
}

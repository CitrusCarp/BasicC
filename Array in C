#include <stdio.h>
main()
{
    int a[4]={2,4,6,1};
    printf("before changing=%d%d%d%d",a[0],a[1],a[2],a[3]);
    a[1]=5;
    a[0]=a[3];
    printf("\nafter changing=%d%d%d%d",a[0],a[1],a[2],a[3]);
}


#include <stdio.h>
main()
{
    int a[4];
    a[0]=2;
    a[1]=4;
    a[2]=6;
    a[3]=1;
    printf("before changing=%d%d%d%d",a[0],a[1],a[2],a[3]);
    a[1]=5;
    a[0]=a[3];
    printf("\nafter changing=%d%d%d%d",a[0],a[1],a[2],a[3]);
}

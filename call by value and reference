#include <stdio.h>
void swap(int x,int y)
{
    int temp;
    temp=x;
    x=y;
    y=temp;
    printf("after swapping\na=%d\nb=%d",x,y);
}
main()
{
    int a,b;
    printf("enter a and b=",a,b);
    scanf("%d%d",&a,&b);
    swap(a,b);
}




#include <stdio.h>
void swap(int *x,int *y)
{
    int temp;
    temp=*x;
    *x=*y;
    *y=temp;
    printf("after swapping\na=%d\nb=%d",*x,*y);
}
main()
{
    int a,b;
    printf("enter a and b=",a,b);
    scanf("%d%d",&a,&b);
    swap(&a,&b);
}

#include <stdio.h>
main()
{
    int n;
    printf("enter no. between 1-5=");
    scanf("%d",&n);
    switch(n)
    {
        case 1:
        printf("you entered 1");
        break;
        
        case 2:
        printf("you entered 2");
        break;
        
        case 3:
        printf("you entered 3");
        break;
        
        case 4:
        printf("you entered 4");
        break;
        
        case 5:
        printf("you entered 5");
        break;
        
        default:
        printf("invalid no.");
        break;
        
    }
    
}
